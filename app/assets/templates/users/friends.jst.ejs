<div class="container">
  <div class="row">
    <div class="col-xs-10 col-xs-offset-1">
      <h1 class="div-title">Friends</h1>

      <div class="bordered-table">
        <div id='new-friendship'></div>

        <% friends.forEach(function(friend){ %>
          <ul>
            <li>
              <%= friend.escape('f_name') + ' ' + friend.escape('l_name') + ' ' %>
              (<%= friend.escape('email') %>)
            </li>
          </ul>
        <% }) %>
      </div>
    </div>
  </div>
</div>
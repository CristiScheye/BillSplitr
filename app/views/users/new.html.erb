<h1>Sign Up</h1>

<%= form_for @user do |f| %>
  <%= f.label :f_name, 'First Name' %>
  <%= f.text_field :f_name %>

  <%= f.label :l_name, 'Last Name' %>
  <%= f.text_field :l_name %>

  <%= f.label :email, 'Email (will be used for login)' %>
  <%= f.email_field :email %>

  <%= f.label :password, 'Password' %>
  <%= f.password_field :password %>

  <%= f.submit 'Sign Up' %>
<% end %>